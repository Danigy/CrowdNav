[env]
time_limit = 25
time_step = 0.1
val_size = 100
test_size = 500
randomize_attributes = false
draw_screen = true
show_sensors = false

[reward]
success_reward = 1
collision_penalty = -0.25
#discomfort_dist = 0.2
discomfort_dist = 1.2
discomfort_penalty_factor = 0.5
potential_reward_weight = 1.0

[sim]
train_val_sim = hallway_crossing
test_sim = hallway_crossing
square_width = 8
circle_radius = 4
hallway_width = 2
human_num = 2

[humans]
visible = true
policy = orca
radius = 0.3
v_pref = 1
sensor = coordinates
personal_space = 1.2
max_linear_velocity = 1.3
max_angular_velocity = 2.0

[robot]
visible = true
policy = multi_human_rl
radius = 0.3
v_pref = 1
sensor = coordinates
personal_space = 1.2
max_linear_velocity = 0.5
max_angular_velocity = 1.0


